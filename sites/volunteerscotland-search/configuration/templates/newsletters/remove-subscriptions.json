{
{{#eachJoin (split @root.request.params.list_ids ',') ','}}
"list-{{this}}":
{{#blockStringify this}}
{
   "method": "POST",
   "url": "\{{@root.secrets.vs_campaign_monitor.baseURL}}subscribers/{{this}}/unsubscribe.json",
   "body": {{> newsletter_post_body}},
   "auth": {"user" : "\{{@root.secrets.vs_campaign_monitor.auth.user}}"},
   "json": true
}
{{/blockStringify}}
{{/eachJoin}}
}
