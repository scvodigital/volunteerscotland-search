{
  "name": "remove-newsletter-subscription",
  "pattern": "/remove-newsletter-subscription/:list_ids",
  "acceptedVerbs": "POST",
  "defaultParams": {
    "context": "results"
  },
  "tasks": [
    "context.metaData.commonTasks.authRequired",
    "context.metaData.commonTasks.getSubscriptionPostBody",
    {
      "name": "remove-subscription",
      "taskModule": "request",
      "config": {
        "optionsTemplates": ">context.metaData.handlebars.newsletters.remove-subscriptions"
      },
      "renderer": "handlebars"
    },
    {
      "name": "destination",
      "taskModule": "render",
      "renderer": "handlebars",
      "config": {
        "output": "body",
        "template": "Removed Subscription"
      }
    }
  ]
}