{
  "name": "testpage",
  "debug": true,
  "pattern": "/testpage/:list_id",
  "acceptedVerbs": "GET",
  "defaultParams": {
    "context": "results"
  },
  "metaData" : {
    "emailTypes" : ["user-confirmation", "register-interest"]
  },
  "tasks": [
    "context.metaData.commonTasks.authNotRequired",
    "context.metaData.commonTasks.allNewsletterInfo",
    "context.metaData.commonTasks.getRawUserSubscriptions",
    "context.metaData.commonTasks.userNewsletterSubscriptions",
    "context.metaData.commonTasks.getSubscriptionPostBody",
    {
      "name": "destination",
      "taskModule": "renderLayout",
      "renderer": "handlebars",
      "config": {
        "logic": {
          "if": [
            "default"
          ]
        },
        "output": "body",
        "layouts": {
          "default": {
            "layout": ">context.metaData.handlebars.layouts.default",
            "parts": {
              "head": ">context.metaData.handlebars.templates.pages_page-head-tags",
              "body": "",
              "foot": ""
            },
            "contentType": "text/html"
          }
        }
      }
    }

  ]
}