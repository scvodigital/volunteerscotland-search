{
  "name": "testpage",
  "debug": true,
  "pattern": "/testpage/:list_ids",
  "acceptedVerbs": "GET",
  "defaultParams": {
    "context": "results"
  },
  "metaData" : {
    "emailTypes" : ["user-confirmation", "register-interest"]
  },
  "tasks": [
    "context.metaData.commonTasks.authNotRequired",
    "context.metaData.commonTasks.allNewsletterInfo",
    {
      "name": "remove-subscription",
      "taskModule": "render",
      "config": {
        "template": ">context.metaData.handlebars.templates.newsletters_remove-subscriptions",
        "parseJson": true,
        "output" : "body"

      },
      "renderer": "handlebars"
    }
  ]
}