{
  "name": "testpage",
  "debug": true,
  "pattern": "/testpage/:list_id",
  "acceptedVerbs": "GET",
  "defaultParams": {
    "context": "results"
  },
  "metaData" : {
    "emailTypes" : ["user-confirmation", "register-interest"]
  },
  "tasks": [
    "context.metaData.commonTasks.authNotRequired",
    "context.metaData.commonTasks.allNewsletterInfo",
    "context.metaData.commonTasks.getRawUserSubscriptions",
    "context.metaData.commonTasks.userNewsletterSubscriptions",
    "context.metaData.commonTasks.getSubscriptionPostBody",
    {
      "name": "remove-subscription",
      "taskModule": "render",
      "config": {
        "optionsTemplates": ">context.metaData.handlebars.newsletters.remove-subscriptions"
      },
      "renderer": "handlebars",
      "output" : "body"
    }
  ]
}